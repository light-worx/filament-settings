{
    "name": "light-worx/filament-settings",
    "description": "Filament Settings",
    "keywords": [
        "light-worx",
        "laravel",
        "filament-settings"
    ],
    "homepage": "https://github.com/light-worx/filament-settings",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Bishop",
            "email": "admin@lightworx.co.za",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "bezhansalleh/filament-shield": "^4.0",
        "filament/filament": "^4.0",
        "stechstudio/filament-impersonate": "^4.0"
    },
    "require-dev": {
        "laravel/pint": "^1.13.1",
        "nunomaduro/collision": "^7.8.1|^8.0",
        "orchestra/testbench": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Lightworx\\FilamentSettings\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "scripts": {
        
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lightworx\\FilamentSettings\\FilamentSettingsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
